{
    "manifest_version": 2, //固定的
    "name": "blankTab", //插件名称
    "version": "1.0", //插件使用的版本
    "description": "一个空白的新标签页", //插件的描述
    "icons": {"16": "blankTab_16.png", "48": "blankTab_48.png", "128": "blankTab_128.png"},
    "browser_action": { //插件加载后生成图标
        "default_icon": "blankTab.png",//图标的图片
        "default_title": "新标签页"//鼠标移到图标显示的文字
        // "default_popup": "blankTab.html" //单击图标执行的文件
    },
    "chrome_url_overrides":{
        "newtab":"blankTab.html"
    },
    "permissions": [ //插件的权限

    ]
}